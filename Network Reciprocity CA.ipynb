    "#List of random grid indicies \n",
    "def CAInds():\n",
    "    CAind = []\n",
    "    perm = itertools.permutations(np.arange(0,K),2) \n",
    "\n",
    "    for i in np.arange(0,K):\n",
    "        CAind.append((i,i))\n",
    "    for i in perm:\n",
    "        CAind.append(i)\n",
    "        \n",
    "    np.random.shuffle(CAind)\n",
    "\n",
    "    return(CAind)\n"
   ]
